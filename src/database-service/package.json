{
  "name": "context-engine-server",
  "version": "0.0.1",
  "description": "context-engine-server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "schema:build": "node -r ts-node/register src/schema/scripts/index.ts",
    "schema:apply": "npm run schema:build && node -r ts-node/register src/schema/scripts/apply-schema.ts",
    "schema:apply:dry-run": "npm run schema:build && node -r ts-node/register src/schema/scripts/apply-schema.ts --dry-run",
    "db:init": "npm run schema:apply",
    "db:init:dry-run": "npm run schema:apply:dry-run",
    "migrate": "node -r ts-node/register src/migrations/run-migrations.ts",
    "migrate:rollback": "node -r ts-node/register src/migrations/run-migrations.ts --rollback",
    "migrate:test": "node -r ts-node/register src/migrations/scripts/test-migration.ts",
    "migrate:debug": "node -r ts-node/register src/migrations/scripts/debug-query.ts",
    "migrate:analyze": "node -r ts-node/register src/migrations/scripts/analyze-schema-dependencies.ts",
    "logs:view": "node -r ts-node/register src/schema/scripts/view-logs.ts",
    "logs:errors": "node -r ts-node/register src/schema/scripts/view-logs.ts --errors",
    "logs:raw": "node -r ts-node/register src/schema/scripts/view-logs.ts --raw",
    "logs:raw-errors": "node -r ts-node/register src/schema/scripts/view-logs.ts --raw --errors",
    "test:schema": "vitest run src/schema/scripts/__tests__/",
    "test:schema:watch": "vitest src/schema/scripts/__tests__/",
    "test:schema:coverage": "vitest run --coverage src/schema/scripts/__tests__/",
    "test:all": "vitest run",
    "test:all:watch": "vitest",
    "test:all:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/livelifelively/context-engine-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/livelifelively/context-engine-server/issues"
  },
  "homepage": "https://github.com/livelifelively/context-engine-server#readme",
  "dependencies": {
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "chalk": "^4.1.2",
    "eslint": "^9.33.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vitest": "^3.2.4"
  }
}
