{
  "name": "context-engine-server",
  "version": "0.0.1",
  "description": "Context Engine Server - DGraph Database and NestJS API",
  "private": true,
  "scripts": {
    "dev": "docker-compose up",
    "dev:build": "docker-compose up --build",
    "dev:down": "docker-compose down",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "logs": "docker-compose logs -f",
    "logs:api": "docker-compose logs -f api-server",
    "logs:dgraph": "docker-compose logs -f alpha",
    "test": "npm run test:db && npm run test:api",
    "test:db": "cd src/database-service && npm test",
    "test:api": "cd src/api-server && npm test",
    "build": "npm run build:db && npm run build:api",
    "build:db": "cd src/database-service && npm run build",
    "build:api": "cd src/api-server && npm run build",
    "install:all": "npm install && cd src/database-service && npm install && cd ../api-server && npm install",
    "clean": "docker-compose down -v && rm -rf node_modules src/*/node_modules src/*/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/livelifelively/context-engine-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/livelifelively/context-engine-server/issues"
  },
  "homepage": "https://github.com/livelifelively/context-engine-server#readme",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.3.2"
  }
}
