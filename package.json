{
  "name": "context-engine-server",
  "version": "0.0.1",
  "description": "Context Engine Server - DGraph Database and NestJS API",
  "private": true,
  "scripts": {
    "dev": "npm run dev:local",
    "dev:local": "npm run dgraph:dev & npm run api:dev",
    "dev:docker": "docker-compose -f docker/compose/full.dev.yml up",
    "dev:build": "docker-compose -f docker/compose/full.dev.yml up --build",
    "dev:down": "docker-compose -f docker/compose/full.dev.yml down",
    "start": "docker-compose -f docker/compose/full.dev.yml up -d",
    "stop": "docker-compose -f docker/compose/full.dev.yml down",
    "logs": "docker-compose -f docker/compose/full.dev.yml logs -f",
    "logs:api": "docker-compose -f docker/compose/full.dev.yml logs -f api-server",
    "logs:dgraph": "docker-compose -f docker/compose/full.dev.yml logs -f alpha",
    "test": "npm run test:db && npm run test:api",
    "test:db": "cd src/database-service && npm test",
    "test:api": "cd src/api-server && npm test",
    "build": "npm run build:db && npm run build:api",
    "build:db": "cd src/database-service && npm run build",
    "build:api": "cd src/api-server && npm run build",
    "install:all": "npm install && cd src/database-service && npm install && cd ../api-server && npm install",
    "clean": "docker-compose -f docker/compose/full.dev.yml down -v && rm -rf node_modules src/*/node_modules src/*/dist",
    "dgraph:dev": "docker-compose -f docker/compose/dgraph.dev.yml up",
    "dgraph:dev:down": "docker-compose -f docker/compose/dgraph.dev.yml down",
    "dgraph:dev:logs": "docker-compose -f docker/compose/dgraph.dev.yml logs -f",
    "api:dev": "cd src/api-server && npm run start:dev",
    "api:dev:debug": "cd src/api-server && npm run start:debug",
    "docker:api": "docker-compose -f docker/compose/api.prod.yml up",
    "docker:api:build": "docker-compose -f docker/compose/api.prod.yml up --build",
    "docker:api:down": "docker-compose -f docker/compose/api.prod.yml down",
    "docker:dgraph": "docker-compose -f docker/compose/dgraph.prod.yml up",
    "docker:dgraph:build": "docker-compose -f docker/compose/dgraph.prod.yml up --build",
    "docker:dgraph:down": "docker-compose -f docker/compose/dgraph.prod.yml down",
    "docker:full:prod": "docker-compose -f docker/compose/full.prod.yml up -d",
    "docker:full:prod:build": "docker-compose -f docker/compose/full.prod.yml up --build -d",
    "docker:full:prod:down": "docker-compose -f docker/compose/full.prod.yml down",
    "docker:full:prod:logs": "docker-compose -f docker/compose/full.prod.yml logs -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/livelifelively/context-engine-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/livelifelively/context-engine-server/issues"
  },
  "homepage": "https://github.com/livelifelively/context-engine-server#readme",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.3.2"
  }
}
